        "from google.colab import files\n",
        "\n",
        "# Upload the file\n",
        "uploaded = files.upload()\n",
        "\n",
        "# Get the filename from the uploaded dict\n",
        "file_path = next(iter(uploaded))\n",
        "\n",
        "# Now use your existing function\n",
        "document = read_file(file_path)\n"
      ],
      "metadata": {
        "id": "Lzx3-PGt1ZC7",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 93
        },
        "outputId": "5adc13f6-98ac-4a1e-e75e-f4a49321d732"
