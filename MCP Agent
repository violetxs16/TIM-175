      "cell_type": "code",
      "source": [
        "def read_file(file):\n",
        "  \"\"\"\n",
        "  read in pdf or txt file\n",
        "  \"\"\"\n",
        "  if file[-3:] == \"txt\":\n",
        "    f = open(file)\n",
        "    return f.read()\n",
        "  elif file[-3:] == \"pdf\":\n",
        "    import pymupdf\n",
        "    text = \"\"\n",
        "    doc = pymupdf.open(file)\n",
        "    for page in doc:\n",
        "      text += page.get_text()\n",
        "    return text\n",
        "\n",
        "  else:\n",
        "    print(\"Please use a pdf or txt file for this task\")\n",
        "    return None\n"
      ],
      "metadata": {
        "id": "epGYGRhc06th"
