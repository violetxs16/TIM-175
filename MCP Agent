        "#You do not need to make any changes to this cell, only run the cell by using the button on the left\n",
        "async def summarize_pdf():\n",
        "    async with resource_app.run() as agent_app:\n",
        "        pdf_reader_agent = Agent(\n",
        "            name=\"pdf_reader\",\n",
        "            instruction=\"You can access and read PDF files from local resources.\",\n",
        "            server_names=[],\n",
        "        )\n",
        "\n",
        "        async with pdf_reader_agent:\n",
        "            # Attach LLM\n",
        "            llm = await pdf_reader_agent.attach_llm(GoogleAugmentedLLM)\n",
        "\n",
        "            # Generate summary\n",
        "            result = await llm.generate_str(\n",
        "                message=f\"\"\"Here is a document: {document[:2000]}\\n\n",
        "                            Summarize it briefly.\"\"\"\n",
        "            )\n",
        "\n",
        "            return result  # ✅ RETURN the result\n"
      ],
      "metadata": {
        "id": "1V7l3dEx4ZxX"
